{
  "name": "express_typescript_typeorm_starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38"
  },
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn ./src/server.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run --config ./src/ormconfig.ts",
    "migration:run:dev": "ts-node ./node_modules/typeorm/cli.js migration:run --config ./src/ormconfig.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert --config ./src/ormconfig.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate --pretty --config ./src/ormconfig.ts -n ",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create --config ./src/ormconfig.ts -n "
  },
  "rootDir": "src"
}
